{
    "version": 2,
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE",
                "OPTIONS"
            ]
        }
    ],
    "functions": {
        "api/test.js": {
          "memory": 3008,
          "maxDuration": 300 
        },
        "api/*.js": {
          "memory": 3008,
          "maxDuration": 300 
        }
    }
}