{
    "version": 2,
    "buildCommand": "npm start",
    "installCommand": "npm install",
    "builds": [
        {
            "src": "dist/src/main.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/src/main.js",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        }
    ]
}